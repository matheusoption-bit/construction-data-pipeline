  "expectativas_focus": {
    "description": "Expectativas de mercado coletadas pelo BCB via Sistema Focus (API OData)",
    "api_base_url": "https://olinda.bcb.gov.br/olinda/servico/Expectativas/versao/v1/odata",
    "endpoints": {
      "selic_12m": {
        "nome": "Expectativa SELIC 12 meses",
        "endpoint": "ExpectativasMercadoSelic",
        "filtro": "$filter=baseCalculo eq '0' and Reuniao eq 'R8'",
        "campos": "Data,Mediana,DataReferencia",
        "frequencia": "diaria",
        "relevancia_bi": "critico",
        "uso": "Projeção de taxa de juros para cálculo de viabilidade futura"
      },
      "ipca_12m": {
        "nome": "Expectativa IPCA 12 meses",
        "endpoint": "ExpectativaMercadoMensais",
        "filtro": "$filter=Indicador eq 'IPCA' and baseCalculo eq '0'",
        "campos": "Data,Mediana,DataReferencia,Indicador",
        "frequencia": "diaria",
        "relevancia_bi": "critico",
        "uso": "Projeção de inflação para atualização de custos"
      },
      "pib_anual": {
        "nome": "Expectativa PIB Anual",
        "endpoint": "ExpectativasMercadoAnuais",
        "filtro": "$filter=Indicador eq 'PIB Total' and baseCalculo eq '0'",
        "campos": "Data,Mediana,DataReferencia,Indicador",
        "frequencia": "semanal",
        "relevancia_bi": "medio",
        "uso": "Cenário macroeconômico para análise de viabilidade"
      },
      "cambio_12m": {
        "nome": "Expectativa Câmbio 12 meses",
        "endpoint": "ExpectativasMercadoAnuais",
        "filtro": "$filter=Indicador eq 'Câmbio' and baseCalculo eq '0'",
        "campos": "Data,Mediana,DataReferencia",
        "frequencia": "semanal",
        "relevancia_bi": "medio",
        "uso": "Projeção de câmbio para materiais importados"
      }
    }
  },
  
