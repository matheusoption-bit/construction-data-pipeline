  "dashboard_bi": {
    "kpis_principais": [
      {
        "nome": "Aquecimento do Mercado",
        "series": ["concessoes_sbpe", "ivg_r"],
        "calculo": "YoY % change",
        "interpretacao": "positivo = aquecido"
      },
      {
        "nome": "Risco do Setor",
        "series": ["inadimplencia_habitacional", "taxa_selic"],
        "calculo": "composite score",
        "interpretacao": "menor = melhor"
      },
      {
        "nome": "Custo de Financiamento",
        "series": ["taxa_media_habitacional", "taxa_selic"],
        "calculo": "spread = taxa_media - selic",
        "interpretacao": "spread alto = mercado apertado"
      },
      {
        "nome": "Pressão de Custos",
        "series": ["incc_di", "ipca_infla"],
        "calculo": "incc/ipca ratio",
        "interpretacao": ">1 = construção pressionada"
      }
    ]
  }
